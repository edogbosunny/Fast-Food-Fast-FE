    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^23.6.0",
    "jwt-decode": "^2.2.0",
    "jest-localstorage-mock": "^2.4.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.3",
    "nyc": "^13.1.0",
    "react-test-renderer": "^16.6.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
